<?xml version="1.0"?>
<robot name="mr_CCMDX1">

<material name="navy">
    <color rgba="0 0 128 50"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 50"/>
  </material>
  <material name="fucsia">
    <color rgba="255 0 255 1"/>
  </material>




  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.2" radius="0.2"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.15"/>
      <material name="fucsia"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.2" radius="0.2"/>
      </geometry>
    <origin rpy="0 0 0" xyz="0 0 0.15"/>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
  </link>



  <link name="right_leg">
    <visual>
      <geometry>
        <box size="0.2 0.1 0.1"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 0"/>
      <material name="black"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.2 0.1 0.1"/>
      </geometry>
      <origin rpy="0 1.57075 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
  </link>
  <joint name="base_to_right_leg" type="fixed">
    <parent link="base_link"/>
    <child link="right_leg"/>
    <origin xyz="0 -0.22 0.18"/>
  </joint>

  <link name="right_round_base">
    <visual>
     <geometry>
      <cylinder length="0.1" radius="0.12"/>
     </geometry>
     <origin rpy="1.57075 1.57075 0" xyz="0 0 0"/>
     <material name="navy"/>
    </visual>
    <collision>
      <geometry>
   <cylinder length="0.1" radius="0.12"/>
      </geometry>
    <origin rpy="1.57075 1.57075 0" xyz="0 0 0"/>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
  </link>
  <joint name="right_round_base_joint" type="fixed">
    <parent link="right_leg"/>
    <child link="right_round_base"/>
    <origin xyz="0 -0.05 -0.09"/>
  </joint>

  <link name="right_box_base">
    <visual>
     <geometry>
       <box size="0.238 0.1 0.14"/>
     </geometry>
     <origin rpy="0 0 0" xyz="0 0 0"/>
     <material name="navy"/>
    </visual>
    <collision>
      <geometry>
       <box size="0.238 0.1 0.14"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="1" ixy="0.0" ixz="0.0" iyy="1" iyz="0.0" izz="1"/>
    </inertial>
  </link>
  <joint name="right_box_ base_joint" type="fixed">
    <parent link="right_round_base"/>
    <child link="right_box_base"/>
    <origin xyz="0 0 -0.05"/>
  </joint>

  <link name="right_front_wheel">
    <visual>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.08" radius="0.035"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin rpy="1.57075 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.08" radius="0.035"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="4"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

    <gazebo reference="$right_front_wheel">
      <mu1 value="200.0"/>
      <mu2 value="100.0"/>
      <kp value="10000000.0" />
      <kd value="1.0" />
      <material>Gazebo/Grey</material>
    </gazebo>

  <joint name="right_front_wheel_joint" type="continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link="right_box_base"/>
    <child link="right_front_wheel"/>
    <origin rpy="0 0 0" xyz="0.08 0 -0.076"/>
  </joint>

  <!-- This block connects the transmission for the robot wheels, joint to an actuator (motor), which informs both simulation and visualization of the robot. For now I dropped xacro  -->

    <transmission name="right_front_wheel_trans">
      <type>transmission_interface/SimpleTransmission</type>
      <actuator name="right_front_wheel_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
      <joint name="right_front_wheel_joint">
        <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
      </joint>
    </transmission>

